class Solution {
public:
    int fib(int n) {
        if (n == 0) {
            return 0;
        }
        if (n == 1) {
            return 1;
        }
        int num1 = 0, num2 = 1, numnew = 0;
        for (int i = 2; i <= n; i++) {
            numnew = num1 + num2;
            num1 = num2;
            num2 = numnew;
        }
        return num2;
    }
};
